# Productive.io API Configuration
# Copy this file to .env and fill in your actual values

# Get your API token from: https://app.productive.io/settings/api
PRODUCTIVE_API_TOKEN=your_api_token_here

# Your organization ID (found in Productive.io URLs)
PRODUCTIVE_ORGANIZATION_ID=your_organization_id_here

# Your user ID (found in Productive.io URLs or API)
PRODUCTIVE_USER_ID=your_user_id_here
